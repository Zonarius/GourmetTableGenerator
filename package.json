{
  "name": "gourmet-table-generator",
  "version": "0.0.1",
  "description": "gourmet-table-generator",
  "license": "MIT",
  "repository": "",
  "author": "",
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty && webpack",
    "buildts": "tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive 'test/**/*-spec.ts'",
    "coverage": "nyc --include='src/**/*.ts' --reporter=text --reporter=html --reporter=lcov mocha --compilers ts:ts-node/register --recursive 'test/**/*.test.ts'",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "watch:webpack": "webpack --watch",
    "webpack": "webpack"
  },
  "dependencies": {
    "es6-promisify": "^5.0.0",
    "fs-extra": "^3.0.1",
    "gourmet-api": "^0.0.3",
    "moment": "^2.18.1",
    "nunjucks": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^3.0.0",
    "@types/fs-extra": "^3.0.0",
    "@types/jquery": "^2.0.43",
    "@types/mocha": "^2.0.0",
    "@types/node": "^7.0.0",
    "@types/nunjucks": "0.0.32",
    "bootstrap": "^3.3.7",
    "chai": "^3.0.0",
    "coveralls": "^2.0.0",
    "datatables.net": "^1.10.15",
    "datatables.net-bs": "^1.10.15",
    "jquery": "^3.2.1",
    "mocha": "^3.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.0.0",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.0",
    "tslint": "^5.0.0",
    "typescript": "^2.0.0",
    "vue": "^2.3.3",
    "webpack": "^2.5.1"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "lib"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}